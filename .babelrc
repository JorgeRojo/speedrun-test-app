{
    "presets": [
        "@babel/env",
        "@babel/react",
        [
            "latest-node",
            {
                "target": "8"
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime", 
            {
              "absoluteRuntime": false,
              "corejs": false,
              "helpers": true,
              "regenerator": true,
              "useESModules": false
            }
        ]
    ]
}